  "scripts": {
    "test:integration": "cross-env DOTENV_CONFIG_PATH=.env.development vitest run src/modules/booking/tests/core-integration.spec.ts",
    "test:config": "vitest run src/modules/booking/tests/config.service.spec.ts src/modules/booking/tests/policy.registry.spec.ts",
    "db:push": "supabase db push",
    "infra:health": "cross-env DOTENV_CONFIG_PATH=.env.development tsx -r dotenv/config src/modules/booking/infrastructure/bootstrap/run-health-check.ts",
    "seed:config:dev": "cross-env DOTENV_CONFIG_PATH=.env.development tsx -r dotenv/config src/modules/booking/infrastructure/configuration/config-seed.script.ts",
    "seed:config:test": "cross-env DOTENV_CONFIG_PATH=.env.test tsx -r dotenv/config src/modules/booking/infrastructure/configuration/config-seed.script.ts",
    "seed:config:prod": "cross-env DOTENV_CONFIG_PATH=.env.production tsx -r dotenv/config src/modules/booking/infrastructure/configuration/config-seed.script.ts",
    "dev": "next dev",
    "test:watch": "cross-env DOTENV_CONFIG_PATH=.env.development vitest",
    "setup:dev": "npm run db:push && npm run seed:config:dev",
    "setup:test": "npm run db:push && npm run seed:config:test"
  }
